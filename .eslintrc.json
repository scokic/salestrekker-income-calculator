{
    "parser": "@babel/eslint-parser",
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended"],
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/prop-types": "off"
    }
}
